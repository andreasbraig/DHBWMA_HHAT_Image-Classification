{
    "filepaths": {
        "good": "Bilder/Good_Pictures",
        "bad": "Bilder/Bad_Pictures",
        "good_gray": "Bilder/Good_Grayscale",
        "bad_gray": "Bilder/Bad_Grayscale",
        "train": "Bilder/train",
        "test":"Bilder/test",
        "validate":"Bilder/validate",
        "new": "Bilder/new"
    },
    "ttsplit": {
        "test_size": 0.2,
        "random_state" : 36
        
    },
    "imagecfg":{
        "FAST_RUN": false,
        "IMG_W": 224,
        "IMG_H": 224,
        "IMG_CH": 1
    },
    "mobilnet":{
        "weights":"imagenet",
        "include_top":false,
        "density1": 1024,
        "density2": 1024,
        "density3": 512,
        "density4": 2,
        "activation_x": "relu",
        "activation_preds": "softmax",
        "layers":20
    },
    "mobilnetv3": {
        "weights": "imagenet",
        "include_top": false,
        "density1": 512,
        "density2": 256,
        "density3": 128,
        "density4": 1,
        "activation_x": "relu",
        "activation_preds": "sigmoid",
        "layers": -30
    },
    "model_compile":{
        "optimizer":"Adam",
        "loss":"categorical_crossentropy",
        "metrics": "accuracy"
    },
    "modelv3_compile":{
        "optimizer":"Adam",
        "loss":"binary_crossentropy",
        "metrics": "accuracy"
    },
    "callbacks":{
        "monitor":"val_accuracy",
        "espatience":10,
        "RLRPpatience":2,
        "verbose":1,
        "factor":0.5,
        "min_lr": 0.00001
    },
    "batch":{
        "size":15,
        "y_col":"label",
        "x_col":"filename",
        "class_mode":"categorical"
    },
    "batch2":{
        "size":15,
        "y_col":"label",
        "x_col":"filename",
        "class_mode":"binary"
    },  
    "train_augmentation": {
        "rotation_range": 20,
        "rescale": 0.00392156862745098,  
        "shear_range": 0.1,
        "zoom_range": 0.2,
        "horizontal_flip": true,
        "width_shift_range": 0.1,
        "height_shift_range": 0.1
    },
    "validation_augmentation": {
        "rescale": 0.00392156862745098  
    },
    "test_augmentation": {
        "rescale": 0.00392156862745098 
    },
    "model":{
        "path": "models/mobilenet_Newset_v1.keras",
        "custom": "models/model30_NEWNET.state"
    }
}
